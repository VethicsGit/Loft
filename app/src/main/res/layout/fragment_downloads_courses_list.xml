<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fl_dcl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="fragment.SettingListFragment">

    <LinearLayout
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/setting_editprofile"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"
            android:src="@drawable/edit_profile" />

        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:text="Edit Profile"
                    android:textSize="20dp" />
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/setting_aboutus"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:padding="@dimen/_5sdp"

            android:layout_marginLeft="5dp"
            android:src="@drawable/about" />
        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:textSize="22dp"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    android:textSize="20dp"
                    android:text="About Us"/>
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_margin="5dp"
        android:id="@+id/setting_feedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"

            android:src="@drawable/feedback" />
        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textSize="20dp"
                    android:layout_gravity="center_vertical"
                    android:text="Send Feedback"/>
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/setting_privacypolice"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"

            android:src="@drawable/privacy" />
        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    android:textSize="20dp"
                    android:text="Privacy Policy"/>
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/setting_termsofuse"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"

            android:src="@drawable/terms" />

        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    android:textSize="20dp"
                    android:text="Terms of Use"/>
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/setting_invitefriends"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"

            android:src="@drawable/invite" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:orientation="vertical">

            <TableRow android:layout_weight="1">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:text="Invite Friends"

                    android:textSize="20dp"
                    />
            </TableRow>

            <TableRow>

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:background="#e0e0e0" />
            </TableRow>
        </TableLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/setting_rateapp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"

            android:src="@drawable/rating" />

        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    android:textSize="20dp"
                    android:text="Rate App"/>
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>



    <LinearLayout
        android:layout_margin="5dp"
        android:id="@+id/setting_logout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:padding="@dimen/_5sdp"

            android:src="@drawable/logout" />
        <TableLayout
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_width = "match_parent"
            android:layout_height = "match_parent">
            <TableRow android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    android:textSize="20dp"
                    android:text="Logout"/>
            </TableRow>
            <TableRow>
                <View
                    android:layout_weight="1"
                    android:layout_gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="#e0e0e0"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>


</LinearLayout>
